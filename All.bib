  abstract	= {An initial value problem for space-time fractional
		  stochastic heat equations driven by colored noise
		  {\$}{\$}{\backslash}partial {\_}t^s u{\_}t +
		  {\backslash}frac{\{}{\backslash}nu {\}}{\{}2{\}} (-
		  {\backslash}Delta )^{\{}{\backslash}alpha /2{\}} u{\_}t =
		  {\backslash}sigma (u{\_}t)
		  {\backslash}dot{\{}W{\}}{\$}{\$}has been discussed in this
		  work. Here, {\$}{\$}{\backslash}partial {\_}t^s{\$}{\$}and
		  {\$}{\$}(- {\backslash}Delta )^{\{}{\backslash}alpha
		  /2{\}}{\$}{\$}stand for the Caputo's fractional derivative
		  of order {\$}{\$}s{\backslash}in (0,1){\$}{\$}and the
		  fractional Laplace operator of order
		  {\$}{\$}{\backslash}alpha {\backslash}in (0,2){\$}{\$},
		  where the second one is also the generator of a strict
		  stable process {\$}{\$}X{\_}t{\$}{\$}of order
		  {\$}{\$}{\backslash}alpha {\$}{\$}with {\$}{\$}E
		  e^{\{}i{\backslash}xi {\backslash}cdot
		  X{\_}t{\}}=e^{\{}-t|{\backslash}xi |^{\backslash}alpha
		  {\}}{\$}{\$}. The nonlinearity {\$}{\$}{\backslash}sigma
		  {\$}{\$}is assumed to be Lipschitz continuous. A
		  formulation of mild random field solutions is obtained due
		  to the called space-time fractional Green functions G, H,
		  where H contains a singular kernel. We focus on studying
		  the spatially--temporally H{\"o}lder continuity of mild
		  random field solutions, which can be obtained by
		  constructing relevant moment bounds for increments of the
		  convolutions {\$}{\$} H {\backslash}otimes b(u) {\$}{\$}and
		  {\$}{\$} H {\backslash}circledast {\backslash}sigma
		  (u){\$}{\$}. Our techniques are based on connecting the
		  space-time fractional Green functions G, H to the
		  fundamental solution of {\$}{\$}{\backslash}partial {\_}t
		  P{\_}t + (-{\backslash}Delta )^{\{}{\backslash}alpha
		  /2{\}}P{\_}t = 0{\$}{\$}, {\$}{\$}P{\_}0={\backslash}delta {\_}0{\$}{\$}via the Wright-type function {\$}{\$}{\{}{\backslash}mathcal {\{}M{\}}{\}}{\_}s{\$}{\$}.},
  issn		= {2190-5444},
  doi		= {10.1140/epjp/s13360-021-01864-4},
  url		= {https://doi.org/10.1140/epjp/s13360-021-01864-4}
}

